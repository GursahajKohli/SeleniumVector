[DEFAULT]
company_name = TD Bank Group
url = https://jobs.td.com/en-CA/job-search-results/?keyword=Ontario&addtnl_categories=Enterprise%%20Data%%20%%26%%20Analytics

# Job list fields
joblist.link.xpath = //div[@class="jobTitle"]/a
joblist.nextpage.css = a[aria-label="Go to the next page of results."]
joblist.pagination = NEXTPAGE

# Job details fields
jobdetails.title.xpath = //h1[@id="gtm-jobdetail-title"]/text()
jobdetails.location.xpath = concat(//span[@id="gtm-jobdetail-city"]/text(), ", ", //span[@id="gtm-jobdetail-state"]/text())
jobdetails.description.xpath = //div[@id="gtm-jobdetail-desc"]
jobdetails.is_url_application = True
jobdetails.logo = https://talenthub.vectorinstitute.ai/uploads/employer/logo/295341/TD_SHIELD_LOGO_COL_RGB.png