[DEFAULT]
company_name = Roche
url = https://www.roche.com/careers/jobs/jobsearch.htm?jobFunctionCodes=JG0020,JG0002&countryCodes=CA

# Job list fields
joblist.link.xpath = //td[contains(@data-title, "Jobdescription")]//a
joblist.pagination = NONE

# Job details fields
jobdetails.title.xpath = //h1[contains(@itemprop, "title")]/text()
jobdetails.location.xpath = //span[contains(@itemprop, "address")]/text()
jobdetails.description.xpath = //div[contains(@data-section-name, "JobDescription")]
jobdetails.is_url_application = True
jobdetails.logo = https://brand.roche.com/dam/jcr:238566d5-6875-43ed-b30f-e62b73d17d91/LOGO%%202%%20ON%%20DARK.png
