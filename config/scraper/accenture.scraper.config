[DEFAULT]
company_name = Accenture
url = https://www.accenture.com/ca-en/careers/jobsearch?jk=Ontario&sb=1&sk=analytics%%20and%%20insights

# Job list fields
joblist.link.xpath = //div[contains(@class, 'job-card-wrapper')]//a
joblist.link.css = div.job-card-wrapper
joblist.nextpage.css = a.next-page-btn:first-of-type
joblist.pagination = NEXTPAGE

# Job details fields
jobdetails.title.xpath = //div[@class="job-position"]/h1/text()
jobdetails.location.xpath = //div[@id="job-listing-hero"]/@data-analytics-job-location
jobdetails.description.xpath = //div[contains(@class, 'job-listing-body')]/div[not(contains(@class, 'apply-page-body'))]
jobdetails.is_url_application = True
jobdetails.logo = https://talenthub.vectorinstitute.ai/uploads/employer/logo/294575/Acc_Logo_Black_Purple_RGB.PNG